<%- include('header') %>
<div class="box" id="heading">
    <h1> <%= backData %> </h1>
</div>

<div class="box">
    <%newWork.forEach(function(number) {%>
    <class class="item">
        <input type="checkbox" name="" id="">
        <p> <%=number%></p>
    </class>

    <%});%>

    <form method="post" action="/">
        <input type="text" name="addWord" placeholder="add new work" id="">
        <button type="submit" name="add" value="<%= backData  %>">+</button>

    </form>
</div>
<%- include('footer') %>